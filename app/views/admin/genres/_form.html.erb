<%= form_with model: [:admin, new_genre], local: false do |f| %>
  <div id="errors">
    <%= render "public/shared/error_messages", resource: new_genre %>
  </div>

  <div class="form-group row">
    <%= f.text_field :name, :placeholder => "ジャンル名"%>
    <%= button_tag type: "submit", class: "btn btn-outline-light btn-sm" do %>
      <i class="fa-solid fa-square-plus fa-2xl" style="color: #FC741A;"></i>
    <% end %>
  </div>
<% end %>